cd /Library/Application\ Support/WLAN/com.realtek.utility.wifi/
grep -rl "0" *rfoff.rtl > MAC

cat MAC | cut -c 1-12 > DEVICE

cat MAC  
exec 6<&0 
exec < MAC
read a1
echo "1" > "$a1"

osascript -e 'quit app "StatusBarApp"'
osascript -e 'open app "StatusBarApp"'
#pkill -f ~/.io.wnuup

exit 0
